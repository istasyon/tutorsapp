<div id="search-results">
  	<%= render "shared/navbar" %>
	<div class="container">
		<% @results.each do |a| %>
			<div class="well well-lg">
				<%= "#{a.user.first_name} #{a.user.last_name}" %><br>
				<%= truncate(a.description, length: 250) %><br>
				<%= "Price: #{a.price} $" %><br>
				<%= link_to "View Schedule", listing_path(a), class: "btn btn-primary" %>
				<%= link_to "Send Message", '#', "data-toggle" => "modal", "data-target" => "#ModalSignUp",class: "btn btn-success" %>			
			</div>
		<% end %>
	</div>
</div>

  <%= render "shared/registration_modals" %>

